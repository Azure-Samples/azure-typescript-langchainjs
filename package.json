{
  "name": "azure-typescript-langchainjs",
  "version": "1.0.0",
  "description": "Azure TypeScript LangchainJS Sample for Azure OpenAI and Azure AI Search.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build --workspaces --if-present && npm link",
    "postbuild": "cd packages/langgraph_agent && npm link && cd ../server_api && npm link langgraph_agent",
    "build:docker": "docker build -t langchain_api .",
    "start": "node packages/server_api/dist/server.js",
    "dev": "npm run dev --workspaces --if-present",
    "start:docker": "npm run build:docker && docker run -p 3000:3000 --env-file ./packages/langgraph_agent/.env langchain_api",
    "dev:studio": "npm run studio -w packages/langgraph_agent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure-Samples/azure-typescript-langchainjs.git"
  },
  "keywords": ["langchainjs", "typescript", "azure", "azure-ai-search"],
  "author": "diberry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Azure-Samples/azure-typescript-langchainjs/issues"
  },
  "homepage": "https://github.com/Azure-Samples/azure-typescript-langchainjs#readme",
  "workspaces": ["packages/*"],
  "dependencies": {
    "typescript": "^5.8.3",
    "dotenv": "^16.4.7",
    "prettier": "^3.5.3"
  }
}
